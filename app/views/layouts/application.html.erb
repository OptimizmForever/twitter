<!DOCTYPE html>
<html>
<head>
  <title>Twitter</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= flash[:notice] %>
<% if current_user %>

  <%= link_to "Выйти",logout_path%>
<%else%>
 <%= link_to "Войти",login_path%>
 <%= link_to "Регистрация",new_user_path%>
<%end%>
<%= yield %>
<%= yield :footer %>
</body>
</html>
